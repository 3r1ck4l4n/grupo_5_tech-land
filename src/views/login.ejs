    <main>
      <article class="tabs">
        <div class="register">
          <a href="/register" class="register-tab">¡Regístrate!</a>
        </div>
        <div class="login">
          <a href="/login" class="login-tab">Ingresa a tu cuenta</a>
        </div>
      </article>
      <section>
        <h1>Iniciar sesión</h1>
        <form action="/home" method="GET">
          <div>
            <input
              size="20px"
              type="text"
              class="input"
              id="fname"
              name="email-address"
              placeholder="  Email"
              required
              minlength="8"
            />
          </div>
          <div>
            <input
              size="20px"
              type="password"
              class="input"
              id="fname"
              name="password"
              placeholder="  Contraseña"
              required
              minlength="8"
              pattern="^(?=.*[A-Za-z])(?=.*\d)(?=.*[@$!%*#?&])[A-Za-z\d@$!%*#?&]{8,}$"
            />
          </div>
          <div class="check">
            <input type="checkbox" class="checkbox" /> ¿Desea guardar su
            contraseña?
          </div>
          <button type="submit" class="register-button">Ingresar</button>
        </form>
      </section>
      <div class="forget"><a href="#">¿Olvidaste tu contraseña?</a></div>
    </main>

